{% extends 'base.html.twig' %}

{% block title %}Hello BandController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">


    {% for band in list %}


    <a href="https://webinfo.iutmontp.univ-montp2.fr/~chaillotv/concertProjectV0/public/band/{{band.id}}"><img src="{{band.picture}}" height="300"  ></a><br>
{{ band.name }}
    {% if not loop.last %}|{% endif %}
{% endfor %}


</div>
{% endblock %}
